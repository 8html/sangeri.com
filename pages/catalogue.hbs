---
layout: cataloguelist.hbs
categories:
  - hotel
  - public
  - restaurant
  - office
  - fixed
  - sofa
permalink: <%= [''].concat(categories).map(function(c) {return '/catalogue/'+(c?c+'/':'');}).join('\n') %>
---
{{#eachIndex posts.catalogue}}
{{#ifCurrentCategory ../permalink item.data.category}}
<p>
  <a href="{{item.data.permalink}}">
  <img class="im im_s" alt="{{item.data.title}}" src="{{item.data.image}}">
  {{item.data.title}}
  </a>
</p>
{{/ifCurrentCategory}}
{{/eachIndex}}
